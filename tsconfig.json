{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/api": ["api"],
      "@/components": ["components"],
      "@/config": ["config"],
      "@/constants/*": ["constants/*"],
      "@/context": ["context"],
      "@/features/*": ["features/*"],
      "@/hocs": ["hocs"],
      "@/hooks": ["hooks"],
      "@/layouts": ["layouts"],
      "@/lib/*": ["lib/*"],
      "@/page-components/*": ["page-components/*"],
      "@/pages/*": ["pages/*"],
      "@/providers": ["providers"],
      "@/public/*": ["public/*"],
      "@/routes": ["routes"],
      "@/styles/*": ["styles/*"],
      "@/utils": ["utils"],
      "@/validators/*": ["validators/*"]
    },
    "allowJs": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": false,
    "pretty": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2018",
    "incremental": true,
    "noImplicitAny": false
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.config.js"],
  "exclude": ["node_modules"]
}
